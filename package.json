{
  "name": "cta-tools",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "autoprefixer": "^10.4.13",
    "eslint-config-custom": "*",
    "postcss": "^8.4.18",
    "prettier": "latest",
    "turbo": "latest"
  },
  "overrides": {
    "typescript": "^4.9.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "npm@8.1.3"
}
